{
    "version": 2,
    "builds": [
        {
            "src": "api/index.php",
            "use": "@vercel/php",
            "config": {
                "installCommand": "apt-get update && apt-get install -y libssl1.0.0"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "outputDirectory": "public"
}
